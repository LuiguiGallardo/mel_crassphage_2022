library("optparse")
library("ggplot2")
library("reshape")
require("gridExtra")
library("ggpubr")
# Data input
table <- read.table("01_adiv_merged/allsamples_average_adiv_comp.txt",
header=TRUE,
sep="\t",
row.names = 1)
# P-value groups
p_value <- list(c("NW", "O"),
c("NW", "OMS"),
c("O", "OMS"))
# Colors
colors <- c("blue",
"orange",
"red")
theme_set(theme_bw())
# Boxplot Observed
boxplot_observed <- ggplot(table,
aes(x = group,
y = observed_otus,
fill=)) +
geom_boxplot(aes(color = group)) +
geom_point(aes(color = group),
size = 1,
shape = 21,
position = position_jitterdodge()) +
labs(x="",
y="Observed crAssphage") +
theme(legend.position = "none") +
scale_color_manual(values = colors) +
stat_compare_means(comparisons = p_value,
method = "wilcox.test",
size = 2)
View(table)
